{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/generate",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/status/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/xyz001",
      "destination": "/api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python@3.9",
      "maxDuration": 15
    }
  }
}
